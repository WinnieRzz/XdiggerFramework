  category="Misc"
  defaultEnabled="true">

  <Dependencies>
    <Addin id="_Shell" version="1.0" />
    <Addin id="_Skeleton" version="1.0" />
  </Dependencies>

  <Runtime>


  </Runtime>



  <Extension path = "/Shell/MainMenu/Tools">
